<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Maratona.Views.GroceryListItemPage"
             Title="Lista - Arrumar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Item da lista de compras" />
            <Entry Placeholder="Nome" />
            <Entry Placeholder="Tipo" />
            <Entry Placeholder="Qtd" />
            <Button Text="Salvar" BorderRadius="35" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>